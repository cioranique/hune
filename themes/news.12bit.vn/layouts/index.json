{"pages":[
    {{ range $index, $page := where .Pages "Params.hidden" "ne" "true" }}
      {{ if ne $page.Type "json" }}
        {{ if $index }},{{ end }}
        {
          "url": "{{  $page.Params.link }}",
          "title": "{{ $page.Params.title }}"
        }
      {{ end }}
    {{ end }}
  ]
  }